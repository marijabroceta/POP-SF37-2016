<Window x:Class="POP_SF_37_2016_GUI.UI.NamestajWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_37_2016_GUI.UI"
        mc:Ignorable="d"
        Title="NamestajWindow" Height="150" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="9*"/>
            <ColumnDefinition Width="40*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="56*"/>
            <RowDefinition Height="65*"/>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" Grid.Column="0" Content="Naziv: " FontSize="15" Grid.ColumnSpan="2" Margin="0,0,-0.4,0"/>
        <TextBox Grid.Row="0" Grid.Column="1" Width="Auto" Name="tbNaziv" />
        <Button Grid.Row="1" Content="Sacuvaj" Grid.Column="1" Margin="0.4,0,186.6,-0.2" Click="SacuvajIzmene"/>
        <Button Grid.Row="1" Content="Izadji" Grid.Column="1" Margin="176.4,0,9.6,-0.2" Click="IzlazIzProzora"/>

    </Grid>
</Window>
