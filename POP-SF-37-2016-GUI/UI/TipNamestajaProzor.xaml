<Window x:Class="POP_SF_37_2016_GUI.UI.TipNamestajaProzor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POP_SF_37_2016_GUI.UI"
        mc:Ignorable="d"
        Title="TipNamestajaProzor" Height="500" Width="425">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="406*"/>
            <RowDefinition Height="59*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox Grid.Row="0" FontSize="20" Name="lbTipNamestaja" Grid.ColumnSpan="4" />
        <Button Content="Dodaj" Grid.Row="1" Grid.Column="0" Click="DodajTipNamestaja" Height="30" Width="60"/>
        <Button Content="Izmeni" Grid.Row="1" Grid.Column="1" Click="IzmeniTipNamestaja" Height="30" Width="60"/>
        <Button Content="Obrisi" Grid.Row="1" Grid.Column="2" Height="30" Width="60"/>
        <Button Content="Zatvori" Grid.Row="1" Grid.Column="3" Click="ZatvoriTipProzora" Height="30" Width="60"/>
    </Grid>
</Window>
